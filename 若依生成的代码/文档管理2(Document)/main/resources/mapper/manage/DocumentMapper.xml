<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iams.manage.mapper.DocumentMapper">
    
    <resultMap type="Document" id="DocumentResult">
        <result property="id"    column="id"    />
        <result property="archiveId"    column="archive_id"    />
        <result property="xuhao"    column="xuhao"    />
        <result property="name"    column="name"    />
        <result property="fileType"    column="file_type"    />
        <result property="fileSize"    column="file_size"    />
        <result property="filePath"    column="file_path"    />
        <result property="fileLocation"    column="file_location"    />
        <result property="content"    column="content"    />
        <result property="authenticity"    column="authenticity"    />
        <result property="integrity"    column="integrity"    />
        <result property="availability"    column="availability"    />
        <result property="security"    column="security"    />
        <result property="createTime"    column="create_time"    />
        <result property="createBy"    column="create_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectDocumentVo">
        select id, archive_id, xuhao, name, file_type, file_size, file_path, file_location, content, authenticity, integrity, availability, security, create_time, create_by, update_time, update_by, remark from tb_document
    </sql>

    <select id="selectDocumentList" parameterType="Document" resultMap="DocumentResult">
        <include refid="selectDocumentVo"/>
        <where>  
            <if test="archiveId != null "> and archive_id = #{archiveId}</if>
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="fileType != null  and fileType != ''"> and file_type = #{fileType}</if>
            <if test="content != null  and content != ''"> and content like concat('%', #{content}, '%')</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
        </where>
    </select>
    
    <select id="selectDocumentById" parameterType="Long" resultMap="DocumentResult">
        <include refid="selectDocumentVo"/>
        where id = #{id}
    </select>

    <insert id="insertDocument" parameterType="Document" useGeneratedKeys="true" keyProperty="id">
        insert into tb_document
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="archiveId != null">archive_id,</if>
            <if test="xuhao != null">xuhao,</if>
            <if test="name != null and name != ''">name,</if>
            <if test="fileType != null">file_type,</if>
            <if test="fileSize != null">file_size,</if>
            <if test="filePath != null">file_path,</if>
            <if test="fileLocation != null">file_location,</if>
            <if test="content != null">content,</if>
            <if test="authenticity != null">authenticity,</if>
            <if test="integrity != null">integrity,</if>
            <if test="availability != null">availability,</if>
            <if test="security != null">security,</if>
            <if test="createTime != null">create_time,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="archiveId != null">#{archiveId},</if>
            <if test="xuhao != null">#{xuhao},</if>
            <if test="name != null and name != ''">#{name},</if>
            <if test="fileType != null">#{fileType},</if>
            <if test="fileSize != null">#{fileSize},</if>
            <if test="filePath != null">#{filePath},</if>
            <if test="fileLocation != null">#{fileLocation},</if>
            <if test="content != null">#{content},</if>
            <if test="authenticity != null">#{authenticity},</if>
            <if test="integrity != null">#{integrity},</if>
            <if test="availability != null">#{availability},</if>
            <if test="security != null">#{security},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateDocument" parameterType="Document">
        update tb_document
        <trim prefix="SET" suffixOverrides=",">
            <if test="archiveId != null">archive_id = #{archiveId},</if>
            <if test="xuhao != null">xuhao = #{xuhao},</if>
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="fileType != null">file_type = #{fileType},</if>
            <if test="fileSize != null">file_size = #{fileSize},</if>
            <if test="filePath != null">file_path = #{filePath},</if>
            <if test="fileLocation != null">file_location = #{fileLocation},</if>
            <if test="content != null">content = #{content},</if>
            <if test="authenticity != null">authenticity = #{authenticity},</if>
            <if test="integrity != null">integrity = #{integrity},</if>
            <if test="availability != null">availability = #{availability},</if>
            <if test="security != null">security = #{security},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteDocumentById" parameterType="Long">
        delete from tb_document where id = #{id}
    </delete>

    <delete id="deleteDocumentByIds" parameterType="String">
        delete from tb_document where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>